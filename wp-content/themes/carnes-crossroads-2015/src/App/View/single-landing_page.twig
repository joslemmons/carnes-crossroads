{#<!--end header from base-->#}

<div class="landing-page">
    <div class="lp-header">
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <div class="logo">
                        <a href="/"><img src="{{ template_uri }}/img/carnes_logo_wht.svg"/></a>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="contact-info">Call Today (843) 761-8600</div>
                        </div>
                        <div class="row btn-row">
                            <div class="col-md-12">
                                <a class="lp-btn" href="http://www.carnescharleston.com">Visit CarnesCrossroads.com</a>
                                <a class="lp-btn" href="http:/www.carnescharleston.com/contact/">Contact Us</a>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="header-content"
         style="background: url('{{ post.getHeaderImage.src }}') 50% 50% no-repeat; background-size: cover;">

        <div class="header-content-container container">
            <h1>{{ post.getTitle }}</h1>
            <div class="title-line"></div>
            <div class="subtitle">{{ post.getSubTitle }}</div>
            {% if post.getHeaderVideoSrc %}
            <div class="video-play play-icon">
                <img src="{{ template_uri }}/img/play-icon.png" alt="Play Video"/>
            </div>
            {% endif %}
        </div>

    </div>

    <div class="page-content--white-floral">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    {{ post.content }}
                </div>
            </div>
        </div>
    </div>

    <div class="featured-content">
        <div class="container">
            <div class="row">
                <div class="col-md-8">
                    <div class="featured-slider">
                        {% for slide in post.getFeatured.slides %}
                        {% if slide.video_src %}
                        <div class="slide">
                            <div class="icon-container">
                                <a href="{{ slide.video_src|youtubeify }}" class="video-play">
                                    <div class="icon-play">
                                        <img src="{{ template_uri }}/img/play-icon_x2.png" alt="Play Video" class="img-responsive"/>
                                    </div>
                                    <img src="{{ slide.image.src|resize(765,466) }}" class="img-responsive"/>
                                </a>
                            </div>
                        </div>
                        {% else %}
                        <div class="slide">
                            <img src="{{ post.getFeatured.image.src }}" class="img-responsive"/>
                        </div>
                        {% endif %}
                        {% endfor %}
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="content-container">
                        <h1>{{ post.getFeatured.title }}</h1>
                        <div class="content">
                            {{ post.getFeatured.content }}
                        </div>
                        {% if post.getFeatured.button and post.getFeatured.button.title %}
                            <a class="site-btn"
                               href="{{ post.getFeatured.button.link }}">{{ post.getFeatured.button.title }}</a>
                        {% endif %}
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="secondary-content">
        <div class="container">
            {% for item in post.getSecondary %}
            <div class="row content-row">
                <div class="col-md-4">
                    <img src="{{ item.image.src|resize(750, 624) }}" class="img-responsive"/>
                </div>
                <div class="col-md-8">
                    <div class="content-container">
                        <div class="secondary-title">
                            <h2>{{ item.title }}</h2>
                        </div>
                        {# <div class="divider"></div> #}
                        <div class="content">
                            {{ item.content }}
                        </div>
                        {% if item.button %}
                            <a class="btn-green" href="{{ item.button.link }}">{{ item.button.title }}</a>
                        {% endif %}
                    </div>
                </div>
            </div>
            {% endfor %}
        </div>
    </div>

    <div class="horizontal-list-content">
        <div class="container">
            <div class="row" >
                {% for item in post.getTertiary %}
                <div class="col-md-3 col-horizontal">
                    <div class="image">
                        <img src="{{ item.image.src|resize(720,573) }}" class="img-responsive">
                    </div>
                    <div class="title">
                        {{ item.title }}
                    </div>
                    <div class="details">
                        {{ item.content|truncate(35) }}
                    </div>
                    {% if item.button %}
                        <a class="btn-green" href="{{ item.button.link }}">{{ item.button.title }}</a>
                    {% endif %}
                </div>
                {% endfor %}
            </div>
        </div>
    </div>

    <div class="footer-content">
        <div class="container-fluid visible-lg visible-md bg-container">
            <div class="row">
                <div class="left-col-bg col-md-6"
                     style="background: url('{{ post.getFooter.image.src }}') 50% 50% no-repeat; background-size: cover;"></div>
                <div class="right-col-bg col-md-6"></div>
            </div>
        </div>
        <div class="container footer-container">
            <div class="row">
                <div class="left-col-text col-md-6">
                    <div class="footer-content">
                        <img src="{{ template_uri }}/img/carnes_logo_realestate_wht.png" alt="Carnes Crossroads Real Estate"/>
                    </div>
                </div>
                <div class="right-col-text col-md-6">
                    <div class="footer-content">
                        <div class="title">{{ post.getFooter.title }}</div>
                        <div class="content">{{ post.getFooter.content }}</div>
                        <div class="lp-form">
                            {{ function('gravity_form', post.getFooter.gravity_form_id, false, false) }}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="page-disclaimer">
        <div class="container">
            <div class="row">
                <div class="col-xs-12">
                    <p>Equal Housing Opportunity. Maps and renderings are representative and may vary from actual construction. Prices and availability subject to change. Chuck Buck â€“ Broker in Charge. &copy; 2016 Carnes Crossroads Real Estate.</p>
                </div>
            </div>
        </div>
    </div>
</div>

{#<!--start footer from base-->#}

{{ wp_footer() }}

        {% if use_ga %}
        <script>
            (function (i, s, o, g, r, a, m) {
                i['GoogleAnalyticsObject'] = r;
                i[r] = i[r] || function () {
                    (i[r].q = i[r].q || []).push(arguments)
                }, i[r].l = 1 * new Date();
                a = s.createElement(o),
                    m = s.getElementsByTagName(o)[0];
                a.async = 1;
                a.src = g;
                m.parentNode.insertBefore(a, m)
            })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

            ga('create', '{{ ga_code }}', 'auto');
            ga('send', 'pageview');
        </script>
        {% endif %}

        {% if use_crazyegg %}
        <script>
            setTimeout(function () {
                var a = document.createElement("script");
                var b = document.getElementsByTagName("script")[0];
                a.src = document.location.protocol + "//script.crazyegg.com/pages/scripts/0018/5125.js?" + Math.floor(new Date().getTime() / 3600000);
                a.async = true;
                a.type = "text/javascript";
                b.parentNode.insertBefore(a, b)
            }, 1);
        </script>
        {% endif %}

